        <url-pattern>/EditRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewRequestStuController</servlet-name>
        <url-pattern>/ViewRequestStu</url-pattern>
    </servlet-mapping>
