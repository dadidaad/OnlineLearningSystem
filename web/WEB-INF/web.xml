<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>LoginFilter</filter-name>
        <filter-class>Filter.LoginFilter</filter-class>
    </filter>
    <filter>
        <filter-name>LogoutFilter</filter-name>
        <filter-class>Filter.LogoutFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>LogoutFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>LoginFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountVerifyController</servlet-name>
        <servlet-class>Controller.AccountVerifyController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpController</servlet-name>
        <servlet-class>Controller.SignUpController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutController</servlet-name>
        <servlet-class>Controller.LogOutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectController</servlet-name>
        <servlet-class>Controller.SubjectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectDescripController</servlet-name>
        <servlet-class>Controller.SubjectDescripController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KnowledgeController</servlet-name>
        <servlet-class>Controller.KnowledgeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAllTeacherController</servlet-name>
        <servlet-class>Controller.ListAllTeacherController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateRequestController</servlet-name>
        <servlet-class>Controller.CreateRequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditRequestController</servlet-name>
        <servlet-class>Controller.EditRequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewRequestStuController</servlet-name>
        <servlet-class>Controller.ViewRequestStuController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ErrorController</servlet-name>
        <servlet-class>Controller.ErrorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeblistController</servlet-name>
        <servlet-class>Controller.WeblistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArticleController</servlet-name>
        <servlet-class>Controller.ArticleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePasswordController</servlet-name>
        <servlet-class>Controller.ChangePasswordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAllRequestStu</servlet-name>
        <servlet-class>Controller.ListAllRequestStuController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountVerifyController</servlet-name>
        <url-pattern>/AccountVerify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpController</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutController</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectController</servlet-name>
        <url-pattern>/Subject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectDescripController</servlet-name>
        <url-pattern>/SubjectDescrip</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KnowledgeController</servlet-name>
        <url-pattern>/Knowledge</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
